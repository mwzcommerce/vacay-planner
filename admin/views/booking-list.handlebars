    <div class="item-container">
        <h2>Sales</h2>
        <div class="filter-container">
            <form class="form-inline">
                <div class="form-group">
                    <label>Country:</label>
                    <select class="form-control form-control-lg country">
                        <option value="">Select Country</option>
                        {{#each countries}}
                            <option value="{{this.countryCode}}">{{this.countryName}}</option>
                        {{/each}}
                    </select>
                </div>
                <div class="form-group">
                    <label>State:</label>
                    <select class="form-control form-control-lg state">
                        <option value="">Select State</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>City:</label>
                    <select class="form-control form-control-lg city">
                        <option value="">Select City</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>Status</label>
                    <select class="form-control form-control-lg">
                        <option value="">Select Status</option>
                    </select>
                </div>
				<label for="from">From</label>
				<input type="text" id="from" name="from">
				<label for="to">to</label>
				<input type="text" id="to" name="to">
            </form>
        </div>
        <br/><br/>
        <table class="cell-border vacay-table" id="bookings">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
					<th>Country Code</th>
                    <th>Email</th>					
                    <th>Contact Number</th>
                    <th>Booking Date</th>
                    <th>Total Amount</th>					
                </tr>
            </thead>
            <tbody>
                {{#each bookingList}}
                    <tr>
                        <td>{{this.firstName}}</td>
                        <td>{{this.lastName}}</td>
						<td>{{this.country_code}}</td>
                        <td>{{this.email}}</td>					
                        <td>{{this.contactNumnber}}</td>
                        <td>{{this.bookingDate}}</td>
                        <td>{{this.totalAmount}}</td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
