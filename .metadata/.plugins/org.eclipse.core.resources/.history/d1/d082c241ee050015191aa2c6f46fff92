package com.vacayplanner;

import org.springframework.context.annotation.AnnotationConfigApplicationContext;

import com.vacayplanner.domain.model.Category;
import com.vacayplanner.repository.CategoryRepository;

/**
 * Hello world!
 *
 */
public class App {
	public static void main(String[] args) {
		AnnotationConfigApplicationContext ctxt = new AnnotationConfigApplicationContext(CategoryRepository.class);
		CategoryRepository repository =  ctxt.getBean(CategoryRepository.class);
		
		repository.save(new Category("Test category I", "Test Description I"));
		repository.save(new Category("Test category II", "Test Description II"));
	}
}
