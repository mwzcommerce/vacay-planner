import org.springframework.context.support.ClassPathXmlApplicationContext;

import com.vacayplanner.repository.CategoryRepository;


public class ApplicationMain {

	public static void main(String[] args) {
		ClassPathXmlApplicationContext ctxt = new ClassPathXmlApplicationContext("/Users/raghavan/Consulting/VacayPlanner/vacay-planner/vacayplanner/src/main/webapp/WEB-INF/vacayplanner-services.xml");
		CategoryRepository repository = ctxt.getBean(CategoryRepository.class);
		
		System.out.println("Repository Lookup Successfully!!!!");
	}

}
